{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/angular",
        "config": {
          "nodeVersion": "22.x",
          "installCommand": "npm install --force",
          "buildCommand": "npm run build",
          "outputDirectory": "dist/board-project/browser"
        }
      },
      {
        "src": "server.ts",
        "use": "@vercel/node",
        "config": {
          "nodeVersion": "22.x"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/$1"
      },
      {
        "src": "/home/:mode/:id",
        "dest": "/"
      }
    ]
  }